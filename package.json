{
  "name": "cash-register-system",
  "version": "1.0.0",
  "description": "Professional cash register system that calculates optimal change using TypeScript. Features REST API, CSV processing, Swagger docs, and Vercel deployment.",
  "main": "dist/src/index.js",
  "scripts": {
    "check-node": "node -e \"const v=process.version.match(/^v(\\d+)/)[1]; if(v<22) throw new Error('Node.js 22 or higher required, found v'+v)\"",
    "prebuild": "npm run check-node && npm run clean",
    "build": "tsc",
    "start": "npm run check-node && node dist/src/api.js",
    "dev": "npm run check-node && ts-node src/api.ts",
    "test": "npm run check-node && jest",
    "lint": "eslint src tests api --ext .ts",
    "lint:fix": "eslint src tests api --ext .ts --fix",
    "format": "prettier --write \"{src,tests,api}/**/*.ts\"",
    "format:check": "prettier --check \"{src,tests,api}/**/*.ts\"",
    "clean": "rm -rf dist",
    "vercel-build": "npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "cash-register",
    "change-calculator",
    "typescript",
    "express",
    "api",
    "csv",
    "swagger",
    "vercel",
    "nodejs",
    "strategy-pattern",
    "greedy-algorithm",
    "clean-architecture"
  ],
  "author": "Professional Development Team",
  "homepage": "https://github.com/username/cash-register-system#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/username/cash-register-system.git"
  },
  "bugs": {
    "url": "https://github.com/username/cash-register-system/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.8.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vercel/node": "^2.3.0",
    "eslint": "^8.53.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.8.1"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
